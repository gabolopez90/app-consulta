    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "sqlite3": "^5.0.2"
  },
  "build": {
    "appId": "consulta",
    "asar": true,
    "win": {
      "target": "NSIS",
      "icon": "build/icon.png"
    }
  },
  "devDependencies": {
    "electron": "^16.0.2",
    "electron-builder": "^22.14.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabolopez90/app-consulta.git"
