{
  "name": "app-consulta",
  "version": "1.0.0",
  "description": "App para consultar la base de datos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -w --x64 --ia32"
  },
  "author": "Gabriel Lopez Olivo <gabolopez1990@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "ejs": "^2.6.1",
